@model Usuario

<div class="form-group">
    <label asp-for="Nome"></label>
    <input asp-for="Nome" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Celular"></label>
    <input asp-for="Celular" class="form-control"/>
</div>
<div class="form-group">
    <label asp-for="Email"></label>
    <input asp-for="Email" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Senha"></label>
    <input asp-for="Senha" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="DataNascimento"></label>
    <input asp-for="DataNascimento" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Genero"></label>
    <select asp-for="Genero" class="form-control" asp-items="@Html.GetEnumSelectList<Genero>()">
        <option>Selecione</option>
    </select>
</div>
<div class="form-group">
    <label asp-for="Pressao"></label>
    <select asp-for="Pressao" class="form-control" asp-items="@Html.GetEnumSelectList<Pressao>()">
        <option>Selecione</option>
    </select>
</div>
<div class="form-group">
    <label asp-for="Temperatura"></label>
    <input asp-for="Temperatura" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Duracao"></label>
    <input asp-for="Duracao" class="form-control" />
</div>



